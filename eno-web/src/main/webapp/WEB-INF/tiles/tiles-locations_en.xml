<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE tiles-definitions PUBLIC
       "-//Apache Software Foundation//DTD Tiles Configuration 2.0//EN"
       "http://tiles.apache.org/dtds/tiles-config_2_0.dtd">
<tiles-definitions>

	<!-- asset layout -->
	 <definition name="locations-layout" extends="main-layout">
	 	<put-attribute name="navigation" value="/WEB-INF/jsp/common/navigationLocations.jsp" />
	 	<put-attribute name="scripts" value="/WEB-INF/jsp/customJs/locations/locationsjs.jsp" />
	 </definition>

   <definition name="locations/*" extends="locations-layout">
      <put-attribute name="title" value="资产管理--位置" />
      <put-attribute name="body" value="/WEB-INF/jsp/locations/{1}.jsp" />
   </definition>
   

</tiles-definitions>